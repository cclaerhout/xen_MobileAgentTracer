    <template title="sedo_agent_account" version_id="1" version_string="1"><![CDATA[	<dl class="ctrlUnit">
		<dt></dt>
		<dd>
			<ul>
				<li>
					<label for="ctrl_allow_sedo_agent">
						<input type="checkbox" name="allow_sedo_agent" value="1" id="ctrl_allow_sedo_agent" {xen:checked "{$visitor.allow_sedo_agent}"} />
						{xen:phrase sedo_agent_user_privacy}
						<p class="hint">{xen:phrase sedo_agent_user_desc}</p>
					</label>
				</li>
				<xen:if is="{$xenOptions.sedo_at_allowraz}">
					<li><input type="button" name="mobile_info_raz" value="{xen:phrase sedo_agent_raz}" class="button OverlayTrigger" data-href="{xen:link 'account/raz_mobileinfo'}" /></li>
				</xen:if>
			</ul>
		</dd>
	</dl>]]></template>
