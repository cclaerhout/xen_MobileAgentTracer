  <template_modifications>
    <modification title="Agent Tracer in Conversations" template_title="conversation_message" execute_order="10" modification_type="preg_replace" callback_class="" callback_method="" description="Add Mobile Agent after Author name" version_id="0" version_string="1" active="1">
      <search_value><![CDATA[#<xen:username.+?class="author"\s/>,#i]]></search_value>
      <replace_value><![CDATA[$0
					<xen:if is="{$xenOptions.sedo_at_auto_conversation} AND {$xenOptions.sedo_at_auto_conversation_style} == 'inline'">
						<xen:include template="sedo_agent">
							<xen:set var="$sedoAtSimple">1</xen:set>
						</xen:include>
					</xen:if>]]></replace_value>
    </modification>
    <modification title="Agent Tracer in Posts" template_title="post" execute_order="10" modification_type="preg_replace" callback_class="" callback_method="" description="Add Mobile Agent after Author name" version_id="0" version_string="1" active="1">
      <search_value><![CDATA[#<xen:username.+?class="author"\s/>,#i]]></search_value>
      <replace_value><![CDATA[$0
					<xen:if is="{$xenOptions.sedo_at_auto_thread} AND {$xenOptions.sedo_at_auto_thread_style} == 'inline'">
						<xen:include template="sedo_agent">
							<xen:map from="$post" to="$message" />
							<xen:set var="$sedoAtSimple">1</xen:set>
						</xen:include>
					</xen:if>]]></replace_value>
    </modification>
    <modification title="Agent Tracer in Profile Comments" template_title="profile_post_comment" execute_order="10" modification_type="preg_replace" callback_class="" callback_method="" description="Add Mobile Agent before date" version_id="0" version_string="1" active="1">
      <search_value><![CDATA[#\s+?<xen:datetime.+?/>#i]]></search_value>
      <replace_value><![CDATA[			<span class="muted">
				<xen:if is="{$xenOptions.sedo_at_auto_profilecomments}">
					<xen:include template="sedo_agent">
						<xen:map from="$comment" to="$message" />
						<xen:set var="$sedoAtSimple">1</xen:set>
					</xen:include>
				</xen:if>
			</span >
$0]]></replace_value>
    </modification>
    <modification title="Agent Tracer in Profile Posts" template_title="profile_post" execute_order="10" modification_type="preg_replace" callback_class="" callback_method="" description="Add Mobile Agent before date" version_id="0" version_string="1" active="1">
      <search_value><![CDATA[#<xen:datetime\stime="\$profilePost\.post_date"\s/>#i]]></search_value>
