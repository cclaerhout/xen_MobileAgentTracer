  <templates>
    <template title="sedo_agent" version_id="10" version_string="1.5"><![CDATA[<xen:if is="{$message.sedo_agent} AND {$visitor.permissions.sedo_mobile_agent_tracer.nview}">

	<xen:if is="{$sedoAtSimple}">
		<xen:require css="sedo_agent_simple.css" />
	<xen:else />
		<xen:require css="sedo_agent.css" />	
		<div class="sedo_at_wrapper">
	</xen:if>

	<xen:if is="{$xenOptions.sedo_at_displaymaxinfo}">

